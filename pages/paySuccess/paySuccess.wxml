<view class="pay">
    <image bindtap='uploadTap' class="zhengmianBtn" src="../../image/pay/bg.png"></image>
    <view class="pay_modul">
        <view class="money">
            <p>
                <!-- <span>￥</span> -->
                300
            </p>
            <p>使用积分:0</p>
        </view>
        <view class="content">
            <image bindtap='uploadTap' class="zhengmianBtn" src="{{formData.positivePhoto}}"></image>
            <view class="p">
                <p>改衣</p>
                <p>
                    <text class="">款式 : </text>
                    <text class="">{{models[formData.oldModel-1]}}{{styles[formData.oldStyle-1]}}</text>
                </p>
                <p>
                    <text class="">尺码 : </text>
                    <text class="">{{formData.newSize}}</text>
                </p>
            </view>
        </view>
    </view>
    <view class="pay_content">
        <p>改衣时间预计为2-3周,请您耐心等待</p>
        <button bindtap='toOrder'>完成</button>
    </view>
</view>